// NAV :: START

-
  var navItemData = [
    {
      name: "About us",
      href: "#"
    },
    {
      name: "Profiles",
      href: "profile.html"
    },
    {
      name: "Logg inn",
      href: "#"
    },
    {
      name: "Add teams",
      href: "team.html"
    }
  ];


mixin navItem(hrefVal, name, activeVal)
  li.nav__item
    a.nav__link(
      href!= hrefVal
      title=""
      class!= (bodyClassName === activeVal) ? "is-active": ""
    )!= name


nav.nav
  ul.nav__list

    +navItem("#", "About us", "")

    if(bodyClassName === 'profile' || bodyClassName === 'team')
      +navItem("profile.htm", "Profiles", "profile")

    +navItem("#", "Logg inn", "")

    if(bodyClassName === 'profile' || bodyClassName === 'team')
      +navItem("team.html", "Add teams", "team")



// NAV :: END